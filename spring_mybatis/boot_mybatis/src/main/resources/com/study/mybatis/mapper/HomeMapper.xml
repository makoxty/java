<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.mybatis.mapper.HomeMapper">

	<insert id="insertClient"
		parameterType="com.study.mybatis.entity.Home" useGeneratedKeys="true"
		keyProperty="id">
		insert into CLIENT (
		name,
		tellNumber,
		sex,
		age
		)
		values
		(
		#{name},
		#{tellNumber},
		#{sex},
		#{age}
		)
	</insert>
</mapper>